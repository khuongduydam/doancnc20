<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider),class:'btn btn-primary',style:'margin:10% 0 0 35%;border-radius:15px;width:80%' %><br />
  <% end -%>
<% end -%>
