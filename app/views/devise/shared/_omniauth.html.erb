<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class:'btn btn-default',style:'width: 80%;border-radius: 15px;background: #aad367;border-color: #aad367;margin-left:35%;;font-family: arial' %><br />
  <% end -%>
<% end -%>
